{
  //--------------- Python Settings ---------------------//
  "pyright.enable": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.stubPath": "typings",
  "python.analysis.typeshedPaths": [],
  "python.analysis.diagnosticSeverityOverrides": {},
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.useLibraryCodeForTypes": true,
  "python.pythonPath": "/usr/bin/python",
  "python.venvPath": "",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/usr/bin/black",
  "python.formatting.blackArgs": [],

  "python.linting.pylintEnabled": true,
  "python.linting.pylintPath": "/usr/bin/pylint",

  //--------------- Lua Settings ------------------------//
  "lua.enable": true,
  "Lua.color.mode": "Semantic",
  "Lua.completion.callSnippet": "Replace",
  "Lua.completion.displayContext": 6,
  "Lua.completion.enable": true,
  "Lua.completion.keywordSnippet": "Disable",
  "Lua.completion.workspaceWord": true,
  "Lua.diagnostics.disable": [],
  "Lua.diagnostics.enable": true,
  "Lua.diagnostics.globals": ["vim"],
  "Lua.diagnostics.neededFileStatus": {},
  "Lua.diagnostics.severity": {},
  "Lua.diagnostics.workspaceDelay": 0,
  "Lua.diagnostics.workspaceRate": 100,
  "Lua.hint.enable": true,
  "Lua.hint.paramName": true,
  "Lua.hint.paramType": true,
  "Lua.hint.setType": true,
  "Lua.hover.enable": true,
  "Lua.hover.fieldInfer": 3000,
  "Lua.hover.previewFields": 100,
  "Lua.hover.viewNumber": true,
  "Lua.hover.viewString": true,
  "Lua.hover.viewStringMax": 1000,
  "Lua.intelliSense.searchDepth": 0,
  "Lua.runtime.nonstandardSymbol": [],
  "Lua.runtime.path": ["?.lua", "?/init.lua", "?/?.lua"],
  "Lua.runtime.special": {
    "include": "require"
  },
  "Lua.runtime.unicodeName": true,
  "Lua.runtime.version": "LuaJIT",
  "Lua.signatureHelp.enable": true,
  "Lua.telemetry.enable": false,
  "Lua.window.progressBar": true,
  "Lua.window.statusBar": true,
  "Lua.workspace.ignoreDir": [".vim"],
  "Lua.workspace.ignoreSubmodules": false,
  "Lua.workspace.library": {
    //    "/usr/share/nvim/runtime/lua": true,
    //    "/usr/share/nvim/runtime/lua/vim/lsp": true
  },
  "Lua.workspace.maxPreload": 20,
  "Lua.workspace.preloadFileSize": 80,
  "Lua.workspace.useGitIgnore": true,

  //----------------------------------------------------//

  //-------------- JavaScript and Friends --------------//

  // DocThis
  "docthis.returnsTag": true,
  "docthis.includeAuthorTag": true,
  "docthis.authorName": "UltiRequiem",
  "docthis.includeDateTag": false,
  "docthis.dateTagFormat": "YYYY-MM-DD",
  "docthis.includeTypes": true,
  "docthis.includeMemberOfOnClassMembers": true,
  "docthis.includeMemberOfOnInterfaceMembers": true,
  "docthis.includeDescriptionTag": false,

  // TSSERVER //
  "tsserver.enable": true,
  "tsserver.locale": null,
  "tsserver.typingsCacheLocation": "",
  "tsserver.formatOnType": true,
  "tsserver.enableJavascript": true,
  "tsserver.maxTsServerMemory": 0,
  "tsserver.tsdk": "",
  "tsserver.npm": "",
  "tsserver.log": "off",
  "tsserver.trace.server": "off",
  "tsserver.pluginPaths": [],
  "tsserver.debugPort": 0,
  "tsserver.watchOptions": {},
  "tsserver.reportStyleChecksAsWarnings": true,
  "tsserver.implicitProjectConfig.checkJs": false,
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "tsserver.disableAutomaticTypeAcquisition": false,
  "tsserver.useBatchedBufferSync": true,
  "typescript.updateImportsOnFileMove.enable": true,
  "typescript.implementationsCodeLens.enable": true,
  "typescript.referencesCodeLens.enable": true,
  "typescript.preferences.importModuleSpecifier": "auto",

  // ESLINT //
  "eslint.packageManager": "npm",
  "eslint.alwaysShowStatus": false,
  "eslint.execArgv": [],
  "eslint.nodeEnv": null,
  "eslint.nodePath": "/usr/bin/node",
  "eslint.options": {},
  "eslint.trace.server": "off",
  "eslint.run": "onType",
  "eslint.autoFixOnSave": false,
  "eslint.quiet": false,
  "eslint.onIgnoredFiles": "warn",
  "eslint.workingDirectories": [],
  "eslint.validate": [],
  "eslint.probe": ["javascript", "typescript", "html", "markdown"],
  "eslint.debug": false,
  "eslint.codeAction.disableRuleComment": {
    "enable": true,
    "location": "separateLine"
  },
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.format.enable": false,
  "eslint.lintTask.options": [],

  //----------------------------------------------------//

  //-------------- HTML and Friends ----------------------//
  "prettier.formatterPriority": 1,
  "prettier.eslintIntegration": true,
  "prettier.requireConfig": false,
  "prettier.ignorePath": ".prettierignore",
  "prettier.statusItemText": "Prettier",
  "prettier.onlyUseLocalVersion": false,
  "prettier.disableLanguages": [],
  "prettier.disableSuccessMessage": false,

  //-------------- Snippets Stuff ----------------------//
  "snippets.priority": 1,
  "snippets.trace": "verbose",
  "snippets.enableStatusItem": false,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets/ultisnippets",
  "snippets.shortcut": "S",
  "snippets.autoTrigger": true,
  "snippets.triggerCharacters": [],
  "snippets.loadFromExtensions": true,
  "snippets.textmateSnippetsRoots": ["~/.config/nvim/snippets/textmate"],
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.usePythonx": true,
  "snippets.ultisnips.directories": [],
  "snippets.snipmate.enable": true,
  "snippets.snipmate.author": "Eliaz Bobadilla",

  //------------------ Git -----------------------//
  "git.addGBlameToVirtualText": true,
  "git.semanticCommit.scope": false,

  //------------------ List -----------------------//
  "lists.disabledLists": [],
  "list.source.files.command": "",
  "list.source.files.args": [],
  "list.source.files.excludePatterns": [],
  "list.source.mru.maxLength": 50,
  "list.source.mru.ignoreGitIgnore": true,
  "list.source.mru.excludePatterns": ["**/.git/*", "/tmp/*"],
  "list.source.grep.useLiteral": true,
  "list.source.grep.command": "rg --vimgrep --no-heading --hidden --smart-case --no-ignore-vcs",
  "list.source.grep.maxColumns": 300,

  //------------------ Integrated Terminal ---------------//
  "terminal.REPLMappings": {
    "c": "cling",
    "cpp": "cling",
    "javascript": "node",
    "typescript": "ts-node",
    "python": "python"
  },
  "terminal.shellPath": "/usr/bin/zsh",
  "terminal.shellArgs": [],

  //------------------ Auto Pairs -----------------------//
  "pairs.disableLanguages": [],
  "pairs.enableCharacters": ["(", "[", "{", "<", "'", "\"", "`"],
  "pairs.enableBackspace": true,
  "pairs.alwaysPairCharacters": [],

  //------------------- CoC Prefreneces -----------------//
  "coc.preferences.extensionUpdateCheck": "weekly",
  "signature.target": "echo",
  "suggest.timeout": 5000,
  "suggest.defaultSortMethod": "none",
  "codeLens.enable": true,
  "coc.source.around.firstMatch": false,
  "coc.source.file.ignoreHidden": false,
  "diagnostic.format": "%message [%source]",
  "diagnostic.virtualText": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.separateRelatedInformationAsDiagnostics": true,
  "coc.preferences.willSaveHandlerTimeout": 750,
  "coc.preferences.enableMarkdown": true,

  // Completion Icons
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": "  ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": " ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  ",
    "default": "  "
  },

  "coc.preferences.colorSupport": true,

  // Diagnostic Symbols
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",

  // Format
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": ["json", "html", "markdown"]

  //----------------------------------------------------//
}
