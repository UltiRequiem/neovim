{
  // Coc Preferences
  "coc.preferences.extensionUpdateCheck": "weekly",
  // Python Stuff
  "python.pythonPath": "/usr/bin/python",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/usr/bin/black",
  "python.linting.pylintEnabled": true,
  "python.linting.pylintPath": "/usr/bin/pylint",
  // Javascript Stuff
  "eslint.nodePath": "/usr/bin/node",
  "prettier.eslintIntegration": true,
  "eslint.autoFixOnSave": true,
  // Snippets Stuff
  "snippets.userSnippetsDirectory": "~/.config/nvim/snipetts/ultisnippets/",
  "snippets.textmateSnippetsRoots": ["~/.config/nvim/snipetts/textmate/"],
  // Git Stuff
  "git.virtualTextPrefix": "",
  "git.addGBlameToVitualText": true,
  "git.addGBlameToBufferVar": true,
  "git.addedSign.text": "+",
  // Diagnostics
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " "
}
